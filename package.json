{
  "name": "misoauto",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "ts-node --esm ./src/index.ts",
    "build": "chmod +x ./scripts/build.sh && ./scripts/build.sh",
    "package": "chmod +x ./scripts/zip.sh && ./scripts/zip.sh",
    "deploy-lambdas": "chmod +x ./scripts/deploy.sh && ./scripts/deploy.sh",
    "prisma-generate": "prisma generate --schema=./services/prisma/schema.prisma",
    "prisma-migrate": "prisma migrate dev --schema=./services/prisma/schema.prisma",
    "prisma-migrate-deploy": "prisma migrate deploy --schema=./services/prisma/schema.prisma",
    "dev": "cd frontend && pnpm dev --port $PORT"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@graphql-tools/graphql-file-loader": "^7.5.16",
    "@graphql-tools/load": "^7.8.13",
    "@graphql-tools/merge": "^8.4.0",
    "@graphql-tools/schema": "^9.0.17",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@edge-runtime/cookies": "^3.0.6",
    "@playwright/test": "^1.32.2",
    "@prisma/client": "^5.2.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/node": "^18.15.11",
    "prettier": "3.0.1",
    "prisma": "^5.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
