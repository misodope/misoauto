{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }, 
    { "source": "/api/(.*)", "destination": "/api/*" }
  ],
  "functions": {
    "api/auth/redirect.ts": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/auth/get.ts": {
      "memory": 512,
      "maxDuration": 15
    },
    "api/auth/tiktok.ts": {
      "memory": 128,
      "maxDuration": 10
    }
  }
}