{
  "name": "misoauto",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "ts-node --esm ./src/index.ts",
    "build": "chmod +x ./scripts/build.sh && ./scripts/build.sh",
    "package": "chmod +x ./scripts/zip.sh && ./scripts/zip.sh",
    "deploy-lambdas": "chmod +x ./scripts/deploy.sh && ./scripts/deploy.sh",
    "migration-up": "node ./services/database/migrate up",
    "migration-down": "node ./services/database/migrate down",
    "prisma-generate": "prisma generate --schema=./services/prisma/schema.prisma",
    "prisma-migrate": "prisma migrate dev --schema=./services/prisma/schema.prisma",
    "prisma-migrate-deploy": "prisma migrate deploy --schema=./services/prisma/schema.prisma",
    "dev": "cd frontend && pnpm dev --port $PORT"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.32.2",
    "@types/aws-lambda": "^8.10.119",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.10.2",
    "dotenv": "^16.0.3",
    "prettier": "3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
